<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Negative CRUD positive Suite">
    <test name="CRUD operations positive tests">
        <classes>
            <class name="com.epam.ta.spec.CRUDTests">
                <methods>
                    <exclude name="getNotExistingPost"/>
                    <exclude name="updateNotExistingPost"/>
